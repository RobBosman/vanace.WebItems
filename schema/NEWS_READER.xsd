<?xml version="1.0" encoding="UTF-8"?><xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">	<xs:element name="NEWS_READER">		<xs:complexType>			<xs:sequence>				<xs:element name="settings_slider">					<xs:complexType>						<xs:attribute name="stroke2Radius" type="xs:byte" use="required"/>						<xs:attribute name="stroke1Radius" type="xs:byte" use="required"/>						<xs:attribute name="totalWidth" type="xs:short" use="required"/>						<xs:attribute name="stroke1Color" type="xs:string" use="required"/>						<xs:attribute name="imageStrokePx" type="xs:byte" use="required"/>						<xs:attribute name="stroke2Color" type="xs:string" use="required"/>						<xs:attribute name="stroke1Px" type="xs:byte" use="required"/>						<xs:attribute name="htmlFieldWidth" type="xs:short" use="required"/>						<xs:attribute name="totalHeight" type="xs:short" use="required"/>						<xs:attribute name="bgRadius" type="xs:byte" use="required"/>						<xs:attribute name="bgColor" type="xs:string" use="required"/>						<xs:attribute name="stroke2Px" type="xs:boolean" use="required"/>					</xs:complexType>				</xs:element>				<xs:element name="settings_menu">					<xs:complexType>						<xs:attribute name="bgHeight" type="xs:byte" use="required"/>						<xs:attribute name="butDistance" type="xs:byte" use="required"/>						<xs:attribute name="bgWidth" type="xs:short" use="required"/>						<xs:attribute name="maskHeight" type="xs:byte" use="required"/>						<xs:attribute name="butHeight" type="xs:byte" use="required"/>					</xs:complexType>				</xs:element>				<xs:element name="news" minOccurs="0" maxOccurs="unbounded">					<xs:complexType>						<xs:sequence>							<xs:element name="description" type="xs:string"/>						</xs:sequence>						<xs:attribute name="target" type="xs:string" use="required"/>						<xs:attribute name="time" use="required">							<xs:simpleType>								<xs:restriction base="xs:NMTOKEN">									<xs:pattern value="[0-9]{2}:[0-9]{2}"/>								</xs:restriction>							</xs:simpleType>						</xs:attribute>						<xs:attribute name="date" use="required">							<xs:simpleType>								<xs:restriction base="xs:NMTOKEN">									<xs:pattern value="[0-9]{2}\-[0-9]{2}\-[0-9]{4}"/>								</xs:restriction>							</xs:simpleType>						</xs:attribute>						<xs:attribute name="buttonText" type="xs:string" use="required"/>						<xs:attribute name="picture" type="xs:string" use="required"/>						<xs:attribute name="title" type="xs:string" use="required"/>						<xs:attribute name="url" type="xs:string" use="required"/>					</xs:complexType>				</xs:element>			</xs:sequence>		</xs:complexType>	</xs:element></xs:schema>