<?xml version="1.0" encoding="UTF-8"?><xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">	<xs:element name="data">		<xs:complexType>			<xs:sequence>				<xs:element name="settings">					<xs:complexType>						<xs:sequence>							<xs:element name="width" type="xs:short"/>							<xs:element name="height" type="xs:short"/>							<xs:element name="visibleItems" type="xs:byte"/>							<xs:element name="itemWidth" type="xs:short"/>							<xs:element name="itemHeight" type="xs:short"/>							<xs:element name="itemsHSpacing" type="xs:byte"/>							<xs:element name="itemsVSpacing" type="xs:byte"/>							<xs:element name="arrowVSpacing" type="xs:byte"/>							<xs:element name="showArrows" type="xs:boolean"/>							<xs:element name="showScrollbar" type="xs:boolean"/>							<xs:element name="scrollbarHeight" type="xs:short"/>							<xs:element name="maxBlur" type="xs:byte"/>							<xs:element name="slideTime" type="xs:decimal"/>							<xs:element name="autoPlay" type="xs:boolean"/>							<xs:element name="autoPlayDelay" type="xs:byte"/>							<xs:element name="pauseOnItemMouseOver" type="xs:boolean"/>							<xs:element name="itemReflection" type="xs:boolean"/>							<xs:element name="reflectionAlpha" type="xs:byte"/>							<xs:element name="reflectionHeight" type="xs:byte"/>							<xs:element name="reflectionDistance" type="xs:boolean"/>						</xs:sequence>					</xs:complexType>				</xs:element>				<xs:element name="news">					<xs:complexType>						<xs:sequence>							<xs:element name="item" minOccurs="0" maxOccurs="unbounded">								<xs:complexType>									<xs:sequence>										<xs:element name="image">											<xs:complexType>												<xs:simpleContent>													<xs:extension base="xs:string">														<xs:attribute name="width" type="xs:short" use="required"/>														<xs:attribute name="height" type="xs:short" use="required"/>													</xs:extension>												</xs:simpleContent>											</xs:complexType>											</xs:element>										<xs:element name="title" type="xs:string"/>										<xs:element name="subTitle" type="xs:string"/>										<xs:element name="content" type="xs:string"/>										<xs:element name="link" type="xs:string" minOccurs="0"/>										<xs:element name="linkTarget" type="xs:string" minOccurs="0"/>									</xs:sequence>								</xs:complexType>							</xs:element>						</xs:sequence>					</xs:complexType>				</xs:element>			</xs:sequence>		</xs:complexType>	</xs:element></xs:schema>